GRANT ALTER , DELETE , INDEX , INSERT , REFERENCES , SELECT , UPDATE
ON ntmtools.tbl_apwsav_hdr_j TO PUBLIC WITH GRANT OPTION ;

GRANT ALTER , DELETE , INDEX , INSERT , REFERENCES , SELECT , UPDATE
ON ntmtools.tbl_apwsav_hdr_j TO QPGMR WITH GRANT OPTION ;

CL: CPYF FROMFILE(QS36F/J.APWSAV) TOFILE(NTMTOOLS/APWSAVHJ) MBROPT(*REPLACE) INCCHAR(*RCD 1 *NE 0000000000I) FMTOPT(*NOCHK)      
