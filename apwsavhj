CREATE TABLE ntmtools.tbl_apwsav_hdr_j
 for system name "APWSAVHJ"
(
	vendor_number                  for AWVEND NUMERIC(5, 0) NOT NULL DEFAULT 0 ,
	voucher_number                 for AWVCH# NUMERIC(5, 0) NOT NULL DEFAULT 0 ,
	record_type                    for AWRTYP CHAR(1) NOT NULL DEFAULT '' ,
	sequence_number                for AWSEQ# NUMERIC(3, 0) NOT NULL DEFAULT 0 ,
	invoice_number                 for AWINV# CHAR(12) NOT NULL DEFAULT '' ,
	manual_cheque                  for AWCHEQ CHAR(1) NOT NULL DEFAULT '' ,
	original_currency_code         for AWOCCD CHAR(2) NOT NULL DEFAULT '' ,
	original_currency_rate         for AWOCRT DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	unused_0001                    for AW0001 CHAR(4) NOT NULL DEFAULT '' ,
	original_currency_amount       for AWOCAM DECIMAL(9, 0) NOT NULL DEFAULT 0 ,
	canadian_funds_amount          for AWCFAM DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	canadian_funds_discount_amount for AWCFDS DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	discount_expiry_date_mdy       for AWEDTE NUMERIC(6, 0) NOT NULL DEFAULT 0 ,
	due_date                       for AWDDTE NUMERIC(6, 0) NOT NULL DEFAULT 0 ,
	purchase_order_number          for AWORD# DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	invoice_date                   for AWIDTE NUMERIC(6, 0) NOT NULL DEFAULT 0 ,
	hold_code                      for AWHOLD CHAR(1) NOT NULL DEFAULT '' ,
	comments                       for AWDESC CHAR(16) NOT NULL DEFAULT '' ,
	distribution_account_1         for AWC01 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_2         for AWC02 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_3         for AWC03 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_4         for AWC04 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_5         for AWC05 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_6         for AWC06 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_7         for AWC07 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_account_8         for AWC08 DECIMAL(7, 0) NOT NULL DEFAULT 0 ,
	distribution_amount_1          for AWT01 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_2          for AWT02 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_3          for AWT03 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_4          for AWT04 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_5          for AWT05 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_6          for AWT06 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_7          for AWT07 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	distribution_amount_8          for AWT08 DECIMAL(9, 2) NOT NULL DEFAULT 0 ,
	posting_date                   for AWPDTE NUMERIC(6, 0) NOT NULL DEFAULT 0 ,
	cheque_number                  for AWCHQ# DECIMAL(5, 0) NOT NULL DEFAULT 0 ,
	cheque_date                    for AWCDTE NUMERIC(6, 0) NOT NULL DEFAULT 0 ,
        docket_1                       for AWDKT1 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_2                       for AWDKT2 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_3                       for AWDKT3 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_4                       for AWDKT4 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_5                       for AWDKT5 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_6                       for AWDKT6 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_7                       for AWDKT7 DECIMAL(7,0) NOT NULL DEFAULT 0,
        docket_8                       for AWDKT8 DECIMAL(7,0) NOT NULL DEFAULT 0,
        tax_status                     for TAXSTS CHAR(8) NOT NULL DEFAULT '',
        unused_38                      for UNUSED CHAR(38) NOT NULL DEFAULT ''
	)
	RCDFMT AWVCHRR    ;

LABEL ON TABLE ntmtools.tbl_apwsav_hdr_j
	IS 'Accounts payable vouchers save file' ;

LABEL ON COLUMN ntmtools.tbl_apwsav_hdr_j
( AWVEND IS 'vendor Number' ,
	AWVCH# IS 'voucher Number' ,
	AWRTYP IS 'record Type' ,
	AWSEQ# IS 'sequence Number' ,
	AWINV# IS 'invoice Number' ,
	AWCHEQ IS 'manual cheque' ,
	AWOCCD IS 'orig currency code' ,
	AWOCRT IS 'orig currency rate' ,
	AW0001 IS 'UNUSED' ,
	AWOCAM IS 'orig currency amt' ,
	AWCFAM IS 'can funds amt' ,
	AWCFDS IS 'can funds disc amt' ,
	AWEDTE IS 'disc exp date MDY' ,
	AWDDTE IS 'due date' ,
	AWORD# IS 'purchase order numb' ,
	AWIDTE IS 'invoice date' ,
	AWHOLD IS 'hold code' ,
	AWDESC IS 'comments' ,
	AWC01 IS 'dist acct 1' ,
	AWC02 IS 'dist acct 2' ,
	AWC03 IS 'dist acct 3' ,
	AWC04 IS 'dist acct 4' ,
	AWC05 IS 'dist acct 5' ,
	AWC06 IS 'dist acct 6' ,
	AWC07 IS 'dist acct 7' ,
	AWC08 IS 'dist acct 8' ,
	AWT01 IS 'dist amt 1' ,
	AWT02 IS 'dist amt 2' ,
	AWT03 IS 'dist amt 3' ,
	AWT04 IS 'dist amt 4' ,
	AWT05 IS 'dist amt 5' ,
	AWT06 IS 'dist amt 6' ,
	AWT07 IS 'dist amt 7' ,
	AWT08 IS 'dist amt 8' ,
	AWPDTE IS 'posting date' ,
	AWCHQ# IS 'cheque number' ,
	AWCDTE IS 'cheque date' ) ;

LABEL ON COLUMN ntmtools.tbl_apwsav_hdr_j
( AWVEND TEXT IS 'vendor Number' ,
	AWVCH# TEXT IS 'voucher Number' ,
	AWRTYP TEXT IS 'record Type' ,
	AWSEQ# TEXT IS 'sequence Number' ,
	AWINV# TEXT IS 'invoice Number' ,
	AWCHEQ TEXT IS 'manual cheque' ,
	AWOCCD TEXT IS 'orig currency code' ,
	AWOCRT TEXT IS 'orig currency rate' ,
	AW0001 TEXT IS 'UNUSED' ,
	AWOCAM TEXT IS 'orig currency amt' ,
	AWCFAM TEXT IS 'can funds amt' ,
	AWCFDS TEXT IS 'can funds disc amt' ,
	AWEDTE TEXT IS 'disc exp date MDY' ,
	AWDDTE TEXT IS 'due date' ,
	AWORD# TEXT IS 'purchase order numb' ,
	AWIDTE TEXT IS 'invoice date' ,
	AWHOLD TEXT IS 'hold code' ,
	AWDESC TEXT IS 'comments' ,
	AWC01 TEXT IS 'dist acct 1' ,
	AWC02 TEXT IS 'dist acct 2' ,
	AWC03 TEXT IS 'dist acct 3' ,
	AWC04 TEXT IS 'dist acct 4' ,
	AWC05 TEXT IS 'dist acct 5' ,
	AWC06 TEXT IS 'dist acct 6' ,
	AWC07 TEXT IS 'dist acct 7' ,
	AWC08 TEXT IS 'dist acct 8' ,
	AWT01 TEXT IS 'dist amt 1' ,
	AWT02 TEXT IS 'dist amt 2' ,
	AWT03 TEXT IS 'dist amt 3' ,
	AWT04 TEXT IS 'dist amt 4' ,
	AWT05 TEXT IS 'dist amt 5' ,
	AWT06 TEXT IS 'dist amt 6' ,
	AWT07 TEXT IS 'dist amt 7' ,
